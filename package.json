{
  "name": "soporte-para-chelang",
  "displayName": "Soporte para CheLang",
  "description": "Soporte para el lenguaje esotérico argentino CheLang.",
  "version": "0.0.1",
  "private": "false",
  "license": "GNU GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/NLGS2907/soporte-vs-para-chelang"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  },
  "contributes": {
    "languages": [{
      "id": "chelang",
      "aliases": ["CheLang", "chelang"],
      "extensions": [".che"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./images/icons/arg-icon.png",
        "dark": "./images/icons/arg-icon.png"
      }
    }],
    "grammars": [{
      "language": "chelang",
      "scopeName": "source.chelang",
      "path": "./syntaxes/chelang.tmLanguage.json"
    }],
    "iconThemes": [
      {
        "id": "chelang",
        "label": "Íconos de Chelang",
        "path": "./iconos/chelang-icon-theme.json"
      }
    ],
    "configuration": {
      "title": "CheLang",
      "properties": {
        "chelang.runArgs.milicos": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Si no están los `--milicos`, no se trabaja los findes ni los feriados."
        }
      }
    },
    "commands": [
      {
        "command": "chelang.runFile",
        "title": "CheLang: Correr Archivo"
      },
      {
        "command": "chelang.printTest",
        "title": "Prueba"
      }
    ]
  }
}
